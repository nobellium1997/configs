function ld
    lsd --color --group-dirs first $argv
    echo
end

function s3dl
    aws s3 cp s3://eds-upstream-intelligence-$argv[1]/$argv[2] $argv[3]
end

function s3ul
    aws s3 cp $argv[2] s3://eds-upstream-intelligence-$argv[1]/$argv[3]
end

function s3ls
    aws s3 ls --recursive s3://eds-upstream-intelligence-$argv
end

function dt
    dotnet test -v m --filter FullyQualifiedName~$argv
end

function dwt
    dotnet watch test -v m --filter FullyQualifiedName~$argv
end

alias ls=ld
alias r=ranger
alias :q=exit
alias ride=rider.sh
alias update="sudo apt update; and sudo apt upgrade -y"
alias grep="grep --color"
alias c="clear"
alias tks="tmux kill-server"
alias open=xdg-open
alias python=python3
alias pip=pip3
alias three=~/.screenlayout/threescreen.sh
alias one=~/.screenlayout/singlescreen.sh
alias ta="tmux attach"
alias cdw="cd ~/Work"
alias t=tig
alias ts="tig status"
alias tref="tig refs"
alias dp="dotnet publish -c Release"
alias dt="dotnet test --filter"
alias ns="notify-send"
alias b="cd .."
alias config='/usr/bin/git --git-dir=/home/nobel/.cfg/ --work-tree=/home/nobel'
alias dbg="fd -e sln -x code {//}"
alias s3d='cd ~/Work/Misc/S3Downloads'
alias proxy="export http_proxy=127.0.0.1:44700; export https_proxy=127.0.0.1:44700"
alias unproxy="set -e http_proxy; set -e https_proxy"
