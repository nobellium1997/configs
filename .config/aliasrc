function getpass
    set -l NAME (bw list items | jq '.[].name' | dmenu)
    set -l NAME_STRIPPED (echo $NAME | sed 's/"//g')
    set -l PASS (bw get password $NAME_STRIPPED)
    set -l USER (bw get username $NAME_STRIPPED)
    echo $USER
    echo $PASS | clip.exe
end
